
//nullable functions
String? stat(String name,int age){
  
  if(age>18) "$name You have accsess";
  else null;
  
}

void main() {
  
 //Null Safty 
  /*normaly we cannot assign null value to variables but using "?" mark 
   after the datatype it can possible*/
  
  int? a=10;
  a=null;
  
  String? name="jack";
  name=null;
  
  print(a);
  print(name);
  
  print(stat("ron",10));
  
  //null collection
  List<int?>? nums=[1,2,3,4,5,null];
  nums=null;
  
}
